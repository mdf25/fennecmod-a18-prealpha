<configs>
	
	<!-- Adds a dependency tag to the start of the XML -->
	<insertBefore xpath="/*/*[1]">
		<!-- DEPENDENCY: FennecModlet - Meds - Additional Meds -->
	</insertBefore>

	<!-- Animal hand items give a chance to get bites. Snakes will always give snake bites. -->
	<append xpath="//item[starts-with(@name, 'meleeHandAnimal') and not(contains(@name, 'Snake'))]//property[@name='Buff']/@value">,buffIllAnimalBitesFM</append>
	<append xpath="//item[starts-with(@name, 'meleeHandAnimal') and not(contains(@name, 'Snake'))]/effect_group[last()]">
		<passive_effect name="BuffProcChance" operation="base_set" value=".5" tags="buffIllAnimalBitesFM" />
	</append>
	<append xpath="//item[@name='meleeHandAnimalSnake']//property[@name='Buff']/@value">,buffIllSnakeBite01FM</append>
	<append xpath="//item[@name='meleeHandAnimalSnake']//property[@name='Buff_chance']/@value">,1</append>
	<append xpath="//item[@name='meleeHandAnimalSnake']/effect_group[last()]">
		<passive_effect name="BuffProcChance" operation="base_set" value="1" tags="buffIllSnakeBite01FM" />
	</append>
	
	
	<!-- Drinking jars of liquid may give hydrophobia buff if player has rabies. -->
	<append xpath="//item[starts-with(@name, 'drinkJar')]">
		<effect_group tiered="false" name="Process Hydrophobia">
			<requirements compare_type="and">
				<requirement name="HasBuff" target="self" buff="buffIllRabies03FM" />
			</requirements>
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="AddBuff" target="self" buff="buffIllHydrophobiaFromDrinkingFM" />
			<triggered_effect trigger="onSelfPrimaryActionEnd" action="RemoveBuff" target="self" buff="buffIllHydrophobiaFromWetnessFM" />
		</effect_group>
	</append>
	
	
	<!-- Allow bandages and first aid bandages to cure treated animal bites -->
	<append xpath="//item[@name='medicalBandage' or @name='medicalFirstAidBandage']">
		<effect_group tiered="false" name="Treat Animal Bites">
			<triggered_effect trigger="onSelfPrimaryActionEnd" target="self" action="AddBuff" buff="buffBandagedAnimalBitesFM">
				<requirement name="HasBuff" target="self" buff="buffTreatedAnimalBitesFM" />
			</triggered_effect>
			<triggered_effect trigger="onSelfSecondaryActionEnd" target="other" action="AddBuff" buff="buffBandagedAnimalBitesFM">
				<requirement name="HasBuff" target="self" buff="buffTreatedAnimalBitesFM" />
			</triggered_effect>
		</effect_group>
	</append>
	
	<!-- First aid kits can cure animal bites to their bandaged state. -->
	<append xpath="//item[@name='medicalFirstAidKit']">
		<effect_group tiered="false" name="Treat Animal Bites">
			<triggered_effect trigger="onSelfPrimaryActionEnd" target="self" action="AddBuff" buff="buffBandagedAnimalBitesFM" />
			<triggered_effect trigger="onSelfPrimaryActionEnd" target="self" action="AddBuff" buff="buffBandagedAnimalBitesFM" />
		</effect_group>
	</append>
	
	<!-- Grain alcohol can now cure animal bites. -->
	<append xpath="//item[@name='drinkJarGrainAlcohol']">
		<property class="Action0">
			<property name="Class" value="Eat"/>
