<configs>
	
	<!-- Adds a dependency tag to the start of the XML -->
	<insertBefore xpath="/*/*[1]">
		<!-- DEPENDENCY: FennecModlet - Entities - Frozen Zombies -->
	</insertBefore>
	
	<!-- Add frozen variants of zombies. -->
	<append xpath="//entity_classes">
		
		<entity_class name="zombieArleneFrozen" extends="zombieArlene">
			<property name="ReplaceMaterial0" value="#@modfolder:Resources/Materials.unity3d?ZombieArleneFrozenMaterial" />
			<property name="SoundFootstepModifier" value="Enemies/Snow_Zombie_Male/zombiesnowfootstep"/>
			<property name="MoveSpeedAggro" value="0.15, 1"/>
		</entity_class>
		
		<entity_class name="zombieArleneFrozenFeral" extends="zombieArleneFeral">
			<property name="ReplaceMaterial0" value="#@modfolder:Resources/Materials.unity3d?ZombieArleneFrozenMaterial" />
