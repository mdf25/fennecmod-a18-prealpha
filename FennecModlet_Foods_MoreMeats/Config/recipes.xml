	<!-- Changes vanilla recipes for any foods needing meat. -->
	
	<!-- Bacon and Eggs: Uses boar meat. Will fail if FennecModlet - Foods - Additional Foods is installed. -->
	<setattribute xpath="//recipe[@name='foodBaconAndEggs']/ingredient[@name='foodRawMeat']" 		name="name">foodRawBoarMeatFM</setattribute>
	<setattribute xpath="//recipe[@name='foodBaconAndEggs']//passive_effect[@tags='foodRawMeat']" 	name="tags">foodRawBoarMeatFM</setattribute>
	
