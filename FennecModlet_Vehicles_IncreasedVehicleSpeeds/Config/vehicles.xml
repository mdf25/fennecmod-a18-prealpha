<configs>
	<!-- Adds a dependency tag to the start of the XML -->
	<insertBefore xpath="/*/*[1]">
		<!-- DEPENDENCY: FennecModlet - Vehicles - Increased Vehicle Speeds -->
	</insertBefore>

	<!-- increases speed of all motor vehicles min and max speed. -->
	<setattribute xpath="//vehicle[@name='vehicleGyrocopter']/property[@name='velocityMax']" name="value">20,30</setattribute>
	<setattribute xpath="//vehicle[@name='vehicleMinibike']/property[@name='velocityMax']" name="value">12,16</setattribute>
	<setattribute xpath="//vehicle[@name='vehicleMotorcycle']/property[@name='velocityMax']" name="value">14,20</setattribute>
	<setattribute xpath="//vehicle[@name='vehicle4x4Truck']/property[@name='velocityMax']" name="value">15,22</setattribute>
	
</configs>
