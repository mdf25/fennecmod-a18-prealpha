<configs>
	
	<!-- Adds a dependency tag to the start of the XML -->
	<insertBefore xpath="/*/*[1]">
		<!-- DEPENDENCY: FennecModlet - World - Increased Zombie Spawns -->
	</insertBefore>
	
	<!-- NOISE SYSTEM -->
	<!-- All noises are louder -->
	<setattribute xpath="//Noise[@ID='1']" name="noise">10</setattribute>
	<setattribute xpath="//Noise[@ID='2']" name="noise">25</setattribute>
	<setattribute xpath="//Noise[@ID='3']" name="noise">60</setattribute>
	<setattribute xpath="//Noise[@ID='4']" name="noise">110</setattribute>
	<setattribute xpath="//Noise[@ID='5']" name="noise">150</setattribute>
	<setattribute xpath="//Noise[@ID='6']" name="noise">30</setattribute>
	
	
	<!-- HEAT SYSTEM OVERHAUL -->
	<!-- Baseline: Turns up all heat map strength sounds -->
	
	<setattribute xpath="//*[@heat_map_strength='0.05']"					name="heat_map_strength">0.25</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.2']" 					name="heat_map_strength">1.0</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.3']" 					name="heat_map_strength">1.5</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.4']" 					name="heat_map_strength">2.0</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.65']"		 			name="heat_map_strength">2.6</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.75']"		 			name="heat_map_strength">3.0</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.8']" 					name="heat_map_strength">4.0</setattribute>
	<setattribute xpath="//*[@heat_map_strength='0.9']" 					name="heat_map_strength">4.5</setattribute>
	<setattribute xpath="//*[@heat_map_strength='1.0']" 					name="heat_map_strength">6.0</setattribute>
	
	
